<?xml version="1.0"?>
<Container version="2">
  <Name>ApachePHP8</Name>
  <Repository>ghcr.io/nightmeer/apachephp8:latest</Repository>
  <Registry>github.com/nightmeer/apachephp8-docker</Registry>
  <Network>br0</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support/>
  <Project/>
  <Overview>ApachePHP8 Docker</Overview>
  <Category>Network:WEB</Category>
  <WebUI>http://[IP]:[PORT:80]</WebUI>
  <TemplateURL/>
  <Icon>https://raw.githubusercontent.com/NightMeer/Unraid-Docker-Templates/main/images/apachephp8.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DonateText/>
  <DonateLink/>
  <Description>ApachePHP8 Docker</Description>
  <Networking>
    <Mode>br0</Mode>
    <Publish/>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/nightmeer_apachephp8/</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Environment/>
  <Labels/>
  <Config Name="Config" Target="/config" Default="/mnt/user/appdata/nightmeer_apachephp8/" Mode="rw" Description="" Type="Path" Display="always" Required="false" Mask="false">/mnt/user/appdata/nightmeer_apachephp8/</Config>
  <Config Name="Port" Target="" Default="80" Mode="tcp" Description="" Type="Port" Display="always" Required="false" Mask="false">80</Config>
